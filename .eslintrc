{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "semi": 0,
    "space-before-function-paren": 0,
    "arrow-parens": 0,
    "operator-linebreak": 0
  },
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  }
}
