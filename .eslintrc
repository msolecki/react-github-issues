{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "semi": 0,
    "space-before-function-paren": 0,
    "arrow-parens": 0,
    "max-len": 0,
    "comma-dangle": [
      "error",
      "only-multiline"
    ],
    "operator-linebreak": 0,
    "react/forbid-prop-types": 0,
    "object-curly-newline": 0
  },
  "env": {
    "es6": true,
    "browser": true,
    "jest": true,
    "node": true
  }
}
